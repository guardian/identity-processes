    "date-fns-tz": "^2.0.0",
    "moment": "^2.29.4",
    "ioredis": "^5.3.2",
    "@aws-sdk/credential-providers": "^3.414.0",
    "@aws-sdk/client-s3": "^3.417.0",
    "@aws-sdk/client-ssm": "^3.414.0"
  },
  "devDependencies": {
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.2",
    "ioredis-mock": "^8.8.3"
  }
}
